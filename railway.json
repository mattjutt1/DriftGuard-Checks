{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "./Dockerfile",
    "buildCommand": null,
    "watchPatterns": ["Dockerfile"]
  },
  "deploy": {
    "startCommand": null,
    "healthcheckPath": "/api/tags",
    "healthcheckTimeout": 600,
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 5,
    "numReplicas": 1
  }
}